                android:layout_marginLeft="1.0dp"
                android:layout_marginTop="1.0dp"
                android:layout_marginRight="1.0dp"
                android:layout_weight="8"
                android:text="灰罐车"
                android:background="#ffffffff"
                android:gravity="center"
                android:textColor="#ff000000" />

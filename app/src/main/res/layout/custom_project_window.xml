<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="112dp"
    android:layout_height="58dp"
    android:background="@color/colorLightGreyMap"
    android:orientation="vertical" >

    <TextView
        android:layout_gravity="center_horizontal"
        android:id="@+id/project_window_text"
        android:layout_marginTop="3dp"
        android:gravity="center"
        android:textSize="13dp"
        android:textColor="@color/colorBlackMap"
        android:layout_width="66dp"
        android:layout_height="17dp" />
 <!--   android:text="陈翔路站工程"代码中动态设置内容-->

    <RelativeLayout
        android:layout_marginTop="8dp"
        android:layout_marginBottom="10dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="106dp"
        android:layout_height="24dp">
        <Button
            android:id="@+id/project_window_button0"
            android:background="@drawable/details_button"
            android:text="项目详情"
            android:textColor="@color/colorLightGreyMap"
            android:textSize="12dp"
            android:layout_alignParentStart="true"
            android:layout_width="50dp"
            android:layout_height="24dp" />

        <Button
            android:id="@+id/project_window_button1"
            android:background="@drawable/details_button"
            android:text="设备动态"
            android:textColor="@color/colorLightGreyMap"
            android:textSize="12dp"
            android:layout_alignParentRight="true"
            android:layout_width="50dp"
            android:layout_height="24dp" />
    </RelativeLayout>
